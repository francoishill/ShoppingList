<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<LinearGradientBrush x:Key='mainCategoryBackground'
						 StartPoint='0,0'
						 EndPoint='0,1'>
		<GradientStop Color='#FFEB832D'
					  Offset='0' />
		<GradientStop Color='#FFFF8827'
					  Offset='0.7' />
		<GradientStop Color='#FFEB832D'
					  Offset='1' />
	</LinearGradientBrush>
	<!--<LinearGradientBrush x:Key='itemBackgroundBrush'
						 StartPoint='0,0'
						 EndPoint='0,1'>
		<GradientStop Color='#FFEB832D'
					  Offset='0' />
		<GradientStop Color='#FFFF8827'
					  Offset='0.7' />
		<GradientStop Color='#FFEB832D'
					  Offset='1' />
	</LinearGradientBrush>-->
	<RadialGradientBrush x:Key='itemBackgroundBrush'>
		<GradientStop Color='#FFEBBF00'
					  Offset='0' />
		<GradientStop Color='#FFD4AE00'
					  Offset='1' />
		<GradientStop Color='#FFFFD00D'
					  Offset='0.36' />
	</RadialGradientBrush>
	<Style x:Key='dateTimeTextBlockStyle'>
		<Setter Property="TextBlock.Foreground"
				Value="#222" />
		<Style.Triggers>
			<Trigger Property="TextBlock.IsMouseOver"
					 Value="True">
				<Setter Property='TextBlock.Foreground'
						Value='White' />
			</Trigger>
		</Style.Triggers>
	</Style>
	<SolidColorBrush x:Key='textblockDoubleclickTextboxForeground'
					 Color='#222' />
</ResourceDictionary>